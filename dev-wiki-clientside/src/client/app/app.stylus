teal-color = #009688;

main-color = teal-color;

main-color-lighten-1 = darken(teal-color, -9%)
main-color-lighten-2 = darken(main-color-lighten-1, -9%)
main-color-lighten-3 = darken(main-color-lighten-2, -9%)
main-color-lighten-4 = darken(main-color-lighten-3, -9%)
main-color-lighten-5 = darken(main-color-lighten-3, -9%)

main-color-darken-1 = darken(teal-color, 9%)
main-color-darken-2 = darken(main-color-darken-1, 9%)
main-color-darken-3 = darken(main-color-darken-2, 9%)
main-color-darken-4 = darken(main-color-darken-3, 9%)
main-color-darken-5 = darken(main-color-darken-3, 9%)





light-blue-3 = #81d4fa
light-blue-2 = #4fc3f7
light-blue-1 = #29b6f6
light-blue   = #03a9f4
light-blue-darken-1 = #039be5
light-blue-darken-2 = #0288d1


main-color = white-color
*
    font-family "Segoe UI", Arial, Helvetica, sans-serif
    -webkit-font-smoothing antialiased

body
    background-color main-color
    overflow hidden
ul
    list-style-type none
    > li
        display list-item
.sample
    transform-origin: center bottom
.container-fluid
    padding-right 0px
    padding-left 0px
.btn
    border-radius 2px
    transition all .3s ease-out
.btn-primary
    background-color main-color-lighten-1
    border-color main-color-lighten-1
    > i
        color white
    &:hover
        background-color main-color-lighten-2
        border-color main-color-lighten-2

*
    text-decoration none !important
